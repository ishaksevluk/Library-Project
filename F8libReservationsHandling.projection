entity F8libReservations {
   reference UserIdRef(UserId) to F8libUsers(UserId);
   reference BookNameRef(BookName) to F8libBookReservationLov(BookName);
   reference AuthorIdRef(AuthorId) to F8libAuthors(AuthorId);
}


